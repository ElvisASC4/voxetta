{
    "name": "voxetta-frontend",
    "version": "0.0.0",
    "description": "Webcomponent voxetta-frontend following open-wc recommendations",
    "author": "voxetta-frontend",
    "license": "MIT",
    "scripts": {
        "start": "es-dev-server --app-index index.html --node-resolve --open --watch",
        "build": "rimraf dist && rollup -c rollup.config.js",
        "start:build": "npm run build && es-dev-server --root-dir dist --app-index index.html --compatibility none --open",
        "format": "prettier src/* --write",
        "lint": "eslint src/*",
        "test": "babel-node jasmine_test/spec/run.js"
    },
    "dependencies": {
        "jasmine-node": "^3.0.0",
        "jsdom": "^16.2.2",
        "lit-element": "^2.0.1",
        "lit-html": "^1.0.0"
    },
    "devDependencies": {
        "@open-wc/building-rollup": "^1.3.1",
        "@open-wc/rollup-plugin-html": "^1.1.0",
        "@rollup/plugin-node-resolve": "^8.1.0",
        "@babel/cli": "^7.10.4",
        "@babel/node": "7.10.4",
        "@babel/preset-env": "7.10.4",
        "@babel/register": "^7.10.4",
        "deepmerge": "^4.2.2",
        "es-dev-server": "^1.55.1",
        "eslint": "^7.3.1",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-lit": "^1.2.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jasmine": "^3.5.0",
        "jasmine-core": "^3.5.0",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.18.0",
        "rollup-plugin-copy": "^3.3.0"
    }
}
