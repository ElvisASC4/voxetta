{
    "extends": ["eslint:recommended", "plugin:jasmine/recommended", "google"],
    "rules": {
        "indent": ["warn", 4, {"SwitchCase": 1}],
        "prettier/prettier": ["warn"],
        "require-jsdoc": ["off"]
    },
    "plugins": ["prettier", "lit", "jasmine"],
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jasmine": true
    }
}
